#pragma string_input true
print "What is your name?\n>>"
copy input reg1

#pragma string_input false
print "What is your favourite number?\n>>"
copy input reg0

print "I see! So your name is "

// Loop to send all bytes in input_ram to output
#pragma char true
label flush_input
print input_ram
add address 1 address
if address <= reg1
goto flush_input
#pragma char false

print " and your favourite number is "
print reg0